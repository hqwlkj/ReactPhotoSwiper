{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ],
    "es2015",
    "react",
    "stage-1" //ES7 grammar support
  ],
  "plugins": [
    "add-module-exports",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
